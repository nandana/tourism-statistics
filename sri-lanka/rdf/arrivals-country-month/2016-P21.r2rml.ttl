  rr:logicalTable [
      rr:tableName "\"2016-P21\""
  ];

  rr:subjectMap [
    a rr:Subject;
    rr:template "http://w3id.org/sri-lanka/tourism/values/arrivals-country-year-month/201601-From-{COUNTRYOFRESIDENCE}";
    rr:termType rr:IRI;
    rr:class qb:Observation;
  ];

  rr:predicateObjectMap[
    rr:predicate qb:dataSet;
    rr:objectMap [
      rr:constant "sltsd:arrivals-month-by-residence";
    ];
  ];

